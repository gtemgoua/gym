{
  "name": "@gym/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node-dev --project tsconfig.dev.json --respawn src/server.ts",
    "build": "tsup src/server.ts --format esm --dts --out-dir dist",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "seed": "ts-node --esm scripts/seed.ts",
    "generate": "prisma generate"
  },
  "dependencies": {
    "@gym/config": "file:../../packages/config",
    "@gym/shared": "file:../../packages/shared",
    "@prisma/client": "^5.12.1",
    "@sinclair/typebox": "^0.32.30",
    "argon2": "^0.31.2",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "morgan": "^1.10.0",
    "pino": "^8.19.0",
    "pino-http": "^8.6.1",
    "stripe": "^14.23.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.1",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.12.7",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-security": "^1.7.1",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.5",
    "prisma": "^5.12.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}
